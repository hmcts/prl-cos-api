{
  "provider": {
    "name": "s2s_auth"
  },
  "consumer": {
    "name": "prl_cos_api"
  },
  "interactions": [
    {
      "description": "a request for a token",
      "request": {
        "method": "POST",
        "path": "/lease",
        "body": {
          "microservice": "prl_cos_api",
          "oneTimePassword": "784467"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "text/plain"
        },
        "body": "someMicroServiceToken",
        "matchingRules": {
          "body": {
            "$": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "microservice with valid credentials"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.11"
    }
  }
}
