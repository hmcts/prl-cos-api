{
  "provider": {
    "name": "feeRegister_lookUp"
  },
  "consumer": {
    "name": "prl_cos"
  },
  "interactions": [
    {
      "description": "A request for PRL Fees",
      "request": {
        "method": "GET",
        "path": "/fees-register/fees/lookup",
        "query": {
          "service": [
            "private law"
          ],
          "channel": [
            "default"
          ],
          "event": [
            "miscellaneous"
          ],
          "jurisdiction2": [
            "family court"
          ],
          "keyword": [
            "ChildArrangement"
          ],
          "jurisdiction1": [
            "family"
          ]
        },
        "matchingRules": {
          "query": {
            "channel": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "default"
                }
              ],
              "combine": "AND"
            },
            "event": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "miscellaneous"
                }
              ],
              "combine": "AND"
            },
            "jurisdiction1": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "family"
                }
              ],
              "combine": "AND"
            },
            "jurisdiction2": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "family court"
                }
              ],
              "combine": "AND"
            },
            "keyword": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "ChildArrangement"
                }
              ],
              "combine": "AND"
            },
            "service": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "private law"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "fee_amount": 232,
          "code": "FEE0336",
          "description": "Section 8 orders (section 10(1) or (2))",
          "version": 2
        },
        "matchingRules": {
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json"
                }
              ],
              "combine": "AND"
            }
          },
          "body": {
            "$.code": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.description": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.version": {
              "matchers": [
                {
                  "match": "number"
                }
              ],
              "combine": "AND"
            },
            "$.fee_amount": {
              "matchers": [
                {
                  "match": "decimal"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "Fees exist for PRL"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.11"
    }
  }
}
